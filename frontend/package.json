{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --ignorePath .gitignore --output src/lib/pathpida --watch",
    "build": "run-s build:path build:next",
    "build:next": "next build",
    "build:path": "pathpida --ignorePath .gitignore --output src/lib/pathpida",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:next": "next lint --dir src --ignore-path .prettierignore --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:typecheck": "tsc --pretty --noEmit",
    "format": "run-s format:eslint format:prettier",
    "format:eslint": "yarn lint:next --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "next": "13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "prettier": "^2.7.1"
  }
}
